}
.sku-dialog {
  background: #fff;
  bottom: 0;
  z-index: 999;
  position: fixed;
  width: 100%;
  padding-bottom: 106rpx;
}
.sku-dialog .proInfo .rent {
  padding: 26rpx;
  display: flex;
  justify-content: flex-start;
  font-size: 24rpx;
  position: relative;
}
.sku-dialog .proInfo .rent .every {
  margin-right: 108rpx;
}
.sku-dialog .proInfo .rent .money {
  color: #FF6000;
  font-size: 32rpx;
}
.sku-dialog .proInfo .rent image {
  width: 80rpx;
  height: 80rpx;
  position: absolute;
  bottom: 34rpx;
  right: 22rpx;
}
.sku-dialog .proInfo .guar {
  background-color: #F6F6F6;
  padding: 15rpx;
  font-size: 24rpx;
  color: #9A9B9C;
  text-align: center;
  display: flex;
  justify-content: flex-start;
}
.sku-dialog .proInfo .guar image {
  width: 24rpx;
  height: 24rpx;
  padding-right: 10rpx;
}
.sku-dialog .proInfo .guar .bloGuar {
  display: inline-block;
}
.sku-dialog .proInfo .guar .bloGuar:first-child {
  margin: 0 50rpx 0 17rpx;
}
.sku-dialog .list {
  padding: 8rpx 26rpx;
  position: relative;
}
.sku-dialog .list .item {
  line-height: 98rpx;
  display: flex;
}
.sku-dialog .list .item:nth-child(4) {
  border-bottom: 1rpx solid #E5E5E5;
  padding-bottom: 20rpx;
}
.sku-dialog .list .title {
  color: #474747;
}
.sku-dialog .list .radio {
  margin-left: 26rpx;
  width: 87%;
  display: flex;
  flex-wrap: wrap;
}
.sku-dialog .list .radio view {
  border: 1rpx solid #B3B3B3;
  width: 150rpx;
  margin-right: 30rpx;
  border-radius: 8rpx;
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 20rpx;
  text-align: center;
}
.sku-dialog .list .radio view.checked {
  border: 1rpx solid #FF6000;
  color: #FF6000;
  background: #FFEFE5;
}
.sku-dialog .list .radio view.disabled {
  background: #f2f2f2;
  color: rgba(51, 51, 51, 0.3);
}
.sku-dialog .good {
  padding: 0 26rpx 20rpx;
}
.sku-dialog .good .title {
  font-size: 28rpx;
  color: #000;
  display: flex;
  justify-content: space-between;
}
.sku-dialog .good .title .price {
  display: flex;
  justify-content: flex-end;
}
.sku-dialog .good image {
  width: 30rpx;
  height: 30rpx;
  margin: -2rpx 14rpx 0 0;
  vertical-align: middle;
  display: inline-block;
}
.sku-dialog .good .radio {
  font-size: 24rpx;
  color: #888;
  margin: 10rpx 0 10rpx 42rpx;
}
.sku-dialog_mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 900;
  color: #fff;
}
.orderModal {
  width: 100%;
