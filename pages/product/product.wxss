}
.orderModal .intro .proDescrip {
  padding: 0 26rpx 30rpx;
}
.orderModal .intro .proDescrip .step {
  margin-top: 34rpx;
}
.orderModal .intro .proDescrip .step image {
  width: 24rpx;
  height: 24rpx;
  margin-right: 16rpx;
}
.orderModal .intro .proDescrip .step .title {
  font-size: 28rpx;
  color: #353535;
}
.orderModal .intro .proDescrip .step .desc {
  margin-top: 8rpx;
  color: #888888;
  font-size: 24rpx;
  margin-left: 40rpx;
}
.orderModal .btn {
  margin-top: 14rpx;
  display: block;
  width: 100%;
  text-align: center;
  padding: 22rpx 0;
  line-height: 60rpx;
  font-size: 36rpx;
  background-color: #fff;
  font-family: MicrosoftYaHei;
}
.service_mask{
  width: 100%;
  height: 100%;
