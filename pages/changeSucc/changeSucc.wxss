
.content .img {
  background: #fff;
  width: 100%;
  text-align: center;
  padding-bottom: 56rpx;
}

.content .img image {
  width: 164rpx;
  height: 164rpx;
  margin-top: 56rpx;
}

.content .img .text {
  font-size: 30rpx;
  color: #474747;
  text-align: center;
  margin: 10rpx 160rpx;
}

.order{
  border-top: 1px solid #E5E5E5;
  background: #fff;
}

.orderDetail {
  display: flex;
  padding: 56rpx 0 20rpx;
  margin: 0 25rpx 20rpx;  
}
 .orderDetail .image {
  width: 195rpx;
  height: 195rpx;
}
 .orderDetail .image image {
  width: 100%;
  height: 100%;
}
 .orderDetail .proInfo {
  color: #888;
  font-size: 24rpx;
  margin: 10rpx 0 0 25rpx;
}
.orderDetail .proInfo .title {
  color: #000;
  font-size: 28rpx;
  padding-bottom: 12rpx;
}
.orderDetail .proInfo view {
  line-height: 34rpx;
}

.date{
  display: flex;
  justify-content: space-between;
  margin: 20rpx 25rpx;
  font-size: 24rpx;
  color: #888;
  padding: 20rpx 0;
  border-top: 1px solid #E5E5E5;
}

.return {
  margin: 60rpx 25rpx;
}

.return .button {
  width: 700tpx;
  height: 94rpx;
  border-radius: 10rpx;
  font-size: 36rpx;
  text-align: center;
  line-height: 94rpx;
}

.returnPage{
  background: #ff6000;
  color: #fff;
}

.detail{
  color: #000;
  background: #fff;
  margin-top: 35rpx;
}

.return .confirm{
  font-size: 24rpx;
  color: #888;
  text-align: center;
  margin: 15rpx 0;
}
